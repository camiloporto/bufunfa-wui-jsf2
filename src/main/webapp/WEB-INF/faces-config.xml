<?xml version="1.0"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee      http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<!--
		Empty for this simple case. Managed bean name automatically derived
		from bean class name. Bean scope is request by default. Results pages
		automatically derived from return value of action controller. From JSF
		2.0 tutorial at http://www.coreservlets.com/JSF-Tutorial/jsf2/
	-->
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>
</faces-config>
